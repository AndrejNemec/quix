			copydirSync_1(tmpFile).to(targetPath);
			rimrafSync_1(tmpFile);
		} else {
			if (filename !== 'degit.json') {
				fs__default['default'].copyFileSync(tmpFile, targetPath);
			}
			fs__default['default'].unlinkSync(tmpFile);
		}
	});
	rimrafSync_1(tmpDir);
}

